{
  "name": "nmrium-cli",
  "version": "0.7.2",
  "description": "CLI commands related to nmrium",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "prepack": "rollup -c",
    "test": "npm run test-only && npm run eslint",
    "test-only": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/nmrium-cli.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/nmrium-cli/issues"
  },
  "homepage": "https://github.com/zakodium/nmrium-cli#readme",
  "jest": {
    "testEnvironment": "node",
    "watchPathIgnorePatterns": [
      "<rootDir>/src/__tests__/data/"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@types/jest": "^29.5.2",
    "eslint": "^8.44.0",
    "eslint-config-cheminfo": "^8.3.1",
    "esm": "^3.2.25",
    "jest": "^29.6.1",
